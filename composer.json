{
  "name": "ghansintecfra/castingfecha",
  "description": "Auxiliar para la conversion de fechas de texto a fechas",
  "type": "library",
  "keywords": [
    "fecha",
    "parseo"
  ],
  "support": {
    "issues": "https://github.com/ghansintecfra/castingfecha/issues",
    "source": "https://github.com/ghansintecfra/castingfecha"
  },
  "authors": [
    {
      "name": "Gabriel Hans Gonzalez",
      "email": "ghans@intecfra.com"
    }
  ],
  "minimum-stable":"dev",
  "prefer-stable":false,
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/ghansintecfra/castingfecha"
    }
  ],
  "require": {
    "php": ">=5.3.3",
    "laravel/framework": "5.*",
    "nesbot/carbon": "^1.22"
  },
  "require-dev": {
    "illuminate/database": "5.5.*",
    "mockery/mockery": "~0.9.4",
    "phpunit/phpunit": "~5.4"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "CastFecha\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "CastFecha\\CastingFechaProvider"
      ],
      "aliases": {
        "ConvertirFecha": "CastFecha\\CastingFechaFacade",
        "Html": "Collective\\Html\\HtmlFacade"
      }
    }
  }
}