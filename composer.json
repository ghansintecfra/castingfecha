{
  "name": "ghans/casting-fecha",
  "description": "Libreria propiedad de intecfa, permite castear las palabras referentes a fechas a una fecha especifica, tambien permite sumar y restar estas a una fecha",
  "type": "library",
  "require": {
    "nesbot/carbon": "^1.22",
    "php": ">=5.5"
  },
  "authors": [
    {
      "name": "Gabriel Hans Gonzalez",
      "email": "ghans@intecfra.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "Intecfra\\CastingFecha\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Intecfra\\CastingFecha\\CastingFechaProvider"
      ],
      "aliases": {
        "Fechas": "Intecfra\\CastingFecha\\CastingFechaFacade"
      }
    }
  }
}
