{
  "name": "ghans/casting-fecha",
  "description": "Libreria propiedad de intecfa, permite castear las palabras referentes a fechas a una fecha especifica, tambien permite sumar y restar estas a una fecha",
  "type": "library",
  "require": {
    "nesbot/carbon": "^1.22",
    "illuminate/support": "5.*",
    "php": ">=5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "authors": [
    {
      "name": "Gabriel Hans Gonzalez",
      "email": "ghans@intecfra.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "CastFecha\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "CastFecha\\CastingFechaProvider"
      ],
      "aliases": {
        "ConvertirFecha": "CastFecha\\CastingFechaFacade",
        "Html": "Collective\\Html\\HtmlFacade"
      }
    }
  }
}
